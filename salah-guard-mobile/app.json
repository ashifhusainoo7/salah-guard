{
  "name": "SalahGuard",
  "displayName": "Smart Salah",
  "expo": {
    "name": "Smart Salah",
    "slug": "salah-guard",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/logoforsalah.png",
    "splash": {
      "backgroundColor": "#0B0F1A"
    },
    "plugins": [
      "./plugins/withDndModule",
      [
        "expo-notifications",
        {
          "ios": {
            "mode": "default"
          }
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoforsalah.png",
        "backgroundColor": "#0B0F1A"
      },
      "package": "com.salahguard",
      "permissions": [
        "ACCESS_NOTIFICATION_POLICY",
        "REQUEST_IGNORE_BATTERY_OPTIMIZATIONS"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "d94487ad-1303-4f65-8c5d-b133e9361d2b"
      }
    },
    "owner": "ashifhusainoo7"
  }
}
